<%@ page language="java" pageEncoding="UTF-8"%><!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"><html xmlns="http://www.w3.org/1999/xhtml"><head><meta http-equiv="X-UA-Compatible" content="IE=Edge"> <%@ include file="../wap/commons/commons_resource_head.jsp"%> <title>会员中心_${comName}</title> <meta charset="utf-8">  <meta name="keywords" content="" />  <meta name="description" content="" />  <meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=2,user-scalable=no">   <link rel="stylesheet" href="${wapBase}/style/mobile/css/common.css">    <link rel="stylesheet" href="${wapBase}/style/mobile/css/font/iconfont.css">     <script src="${wapBase}/style/jquery-1.8.3.min.js"></script>     <script type="text/javascript" src="${wapBase}/style/layer/layer.min.js"></script>     <script type="text/javascript" src="${wapBase}/style/common.js"></script></head><body> <header id="header2"> <h1>收货地址</h1> <div class="back">  <a class="ap-icon" href="javascript:history.back();"></a> </div> <div class="menu">  <!--<a class="icon-cart ap-icon" href="/yunbuy/cart"><em class="cartNum">3</em></a>-->  <a class="icon-home ap-icon" href="/"></a> </div> </header> <link rel="stylesheet" href="${wapBase}/style/mobile/css/member.css">  <div id="content" class="container main">   <div class="tips-m">    编辑收货人信息 <span class="color01">(最多保存5个有效地址)</span> <a href="/member/address" class="color02">[新增]</a>   </div>   <div class="form-m">    <form action="/member/address/save" target="iframeNews" method="post" id="submit_form">     <div class="item ui-clr">      <dl>       <dt class="color03">        <span class="color01">*</span> 配送区域：       </dt>       <dd>        <div id="select_linkage">         <input type="text" style="display: none; width: 70%" placeholder="输入省" id="provinceName" name="provinceName" /> <input type="text"          style="display: none; width: 70%" placeholder="输入市" id="cityName" name="cityName" /> <input type="text" style="display: none; width: 70%" placeholder="输入区"          id="areaName" name="areaName" /> <select id="pf-select-area1" ref="pf-select-area2" name="provinceId" areaId="${address.provinceId}" style="max-width: 8rem;"></select>         <select id="pf-select-area2" ref="pf-select-area3" name="cityId" areaId="${address.cityId}" style="max-width: 8rem;"></select> <select id="pf-select-area3"          name="areaId" areaId="${address.areaId}" style="max-width: 8rem;"></select>        </div>       </dd>      </dl>     </div>     <div class="item ui-clr">      <dl>       <dt class="color03">        <span class="color01">*</span> 详细地址：       </dt>       <dd>        <input name="areaInfo" value="${address.areaInfo }" datatype="*" type="text" class="input-m">       </dd>      </dl>     </div>     <div class="item ui-clr">      <dl>       <dt class="color03">        <span class="color01">*</span> 收货人姓名：       </dt>       <dd>        <input name="trueName" value="${address.trueName }" datatype="*2-6" type="text" class="input-m">       </dd>      </dl>     </div>     <div class="item ui-clr">      <dl>       <dt class="color03">        <span class="color01">*</span> 手机/电话：       </dt>       <dd>        <input name="mobile" value="${address.mobile }" datatype="*2-6" type="text" class="input-m">       </dd>      </dl>     </div>     <div class="item ui-clr">      <dl>       <dt class="color03">邮政编码：</dt>       <dd>        <input name="zip" value="${address.zip }" datatype="n" ignore="ignore" type="text" class="input-m">       </dd>      </dl>     </div>     <div class="item ui-clr">      <dl>       <dt class="color03">设为默认：</dt>       <dd>        <input type="checkbox" style="height: 30px; " name="isDefaultAddress" value="${address.isDefaultAddress }" <c:if test="${address.isDefaultAddress  eq 1 }">checked</c:if>>       </dd>      </dl>     </div>     <input type="hidden" name="back" value="" /> <input type="hidden" name="id" value="${address.id }"> <input name="Submit" type="submit" value="保 存" class="btn" />   </div>   <div class="list01 list-address">    <c:forEach var="address" items="${addressList}" varStatus="v" end="4">     <dl class="item">      <dt>       <b>有效地址${v.index+1}</b>      </dt>      <dd>       <table cellpadding="0" cellspacing="0">        <tbody>         <tr>          <th>收货人</th>          <td>${address.trueName }</td>         </tr>         <tr>          <th>手机/电话</th>          <td>${address.mobile }</td>         </tr>         <tr>          <th>收货地址</th>          <td>${address.cityName }&nbsp;${address.areaName }&nbsp;${address.areaInfo }</td>         </tr>         <tr>          <th>邮编</th>          <td>${address.zip }</td>         </tr>         <tr>          <th>操作</th>          <td><a href="/member/address/${address.id }" class="color02">编辑</a>&nbsp;&nbsp; <a           href="javascript:zz_confirm('您是否确认删除收货地址?','/member/address_del/${address.id }');" class="color02">删除</a></td>         </tr>        </tbody>       </table>      </dd>     </dl>    </c:forEach>   </div>  </div>  <!--  公共底部 -->  <c:set var="footType" value="member" />    <%@ include file="../wap/index/index_foot.jsp"%>  <!--  公共底部 -->  <script type="text/javascript">			var provinceId = "${address.provinceId}";			var cityId = "${address.cityId}";			var areaId = "${address.areaId}";			var $subBox = $("input[name='isDefaultAddress']");			$subBox.click(function() {				$subBox.length == $("input[name='isDefaultAddress']:checked").length ? $(this).val(1) : $(this).val(0);			});		</script>  <script type="text/javascript" src="${mimeBase}/scripts/pub/common/area.js"></script>