<%@ page language="java" pageEncoding="UTF-8"%><!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"><html xmlns="http://www.w3.org/1999/xhtml"><head><meta http-equiv="X-UA-Compatible" content="IE=Edge"> <%@ include file="../wap/commons/commons_resource_head.jsp"%> <title>会员中心_${comName}</title> <meta charset="utf-8">  <meta name="keywords" content="" />  <meta name="description" content="" />  <meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=2,user-scalable=no">   <link rel="stylesheet" href="${wapBase}/style/mobile/css/common.css">    <link rel="stylesheet" href="${wapBase}/style/mobile/css/font/iconfont.css">     <script src="${wapBase}/style/jquery-1.8.3.min.js"></script>     <script type="text/javascript" src="${wapBase}/style/layer/layer.min.js"></script>     <script type="text/javascript" src="${wapBase}/style/common.js"></script></head><body> <header id="header2"> <h1>个人资料</h1> <div class="back">  <a class="ap-icon" href="javascript:history.back();"></a> </div> <div class="menu">  <a class="icon-home ap-icon" href="/"></a> </div> </header> <link rel="stylesheet" href="${wapBase}/style/mobile/css/member.css">  <div id="content" class="container main">   <div class="tab-m-a">    <ul class="ui-clr">     <li class="ta">基本信息</li>     <li class="ta last">修改密码</li>    </ul>   </div>   <div class="tab-m-c" style="display: none;">    <div class="tips-m">     亲爱的 <span class="color01">${user.nickName }</span><br> 请填写您的真实资料，有助于我们更好的为您服务。<!--       <div class="prompt">完善以下资料即可获得80经验值，一元云购不会以任何形式公开您的个人隐私，请放心填写！</div> -->    </div>    <div class="form-m">     <form action="infoSave" target="iframeNews" method="post" id="submit_form">      <div class="item ui-clr">       <dl>        <dt class="color03">昵 称：</dt>        <dd>         <input type="text" class="input-m" name="nickName" value="${user.nickName }" />        </dd>       </dl>      </div>      <div class="item ui-clr">       <dl>        <dt class="color03">手 机：</dt>        <dd>         <input type="text" class="input-m" name="mobile" value="${user.mobile}" />        </dd>      </div>       <div class="item ui-clr">       <dl>        <dt class="color03">真实姓名：</dt>        <dd>         <input type="text" class="input-m" name="trueName" value="${user.trueName}" />        </dd>      </div>       <div class="item ui-clr">       <dl>        <dt class="color03">身份证号码：</dt>        <dd>         <input type="text" class="input-m" name="cardNo" value="${user.cardNo}" />        </dd>      </div>      <div class="item ui-clr">       <dl>        <dt class="color03">电子邮箱：</dt>        <dd>         <input type="text" class="input-m" name="email" value="${user.email}" />        </dd>       </dl>      </div>      <div class="item ui-clr">       <dl>        <dt class="color03">留下一句：</dt>        <dd>         <input name="remark" type="text" class="input-m" value="${user.remark }" />         <div class="tip-m" style="line-height: 1.8; font-size: 14px;">          <a href="../user/${user.id }.html" class="color02">去个人空间看看</a>         </div>        </dd>       </dl>      </div>      <input class="btn" name="Submit" type="submit" value="确认修改">     </form>    </div>   </div>   <div class="tab-m-c" style="display: none;">    <div class="form-m">     <form action="chpass" target="iframeNews" method="post">      <div class="item ui-clr">       <dl>        <dt class="color03">原密码：</dt>        <dd>         <input type="password" class="input-m" name="oldpass" value="" />        </dd>       </dl>      </div>      <div class="item ui-clr">       <dl>        <dt class="color03">新密码：</dt>        <dd>         <input type="password" class="input-m" name="pass1" value="" />        </dd>       </dl>      </div>      <div class="item ui-clr">       <dl>        <dt class="color03">确认密码：</dt>        <dd>         <input type="password" class="input-m" name="pass2" value="" />        </dd>       </dl>      </div>      <input class="btn" name="Submit" type="submit" value="确认修改">     </form>    </div>   </div>  </div>  <script type="text/javascript">			$(function() {				tabs('#content', '.tab-m-a li', '.tab-m-c', 'on', 'click');				if (location.hash == '#pass') {					$('.tab-m-a li').eq(1).click();				}			})		</script>  <!--  公共底部 -->  <c:set var="footType" value="member" />    <%@ include file="../wap/index/index_foot.jsp"%>  <!--  公共底部 --></body></html><script src="${wapBase}/style/mobile/js/jquery.more.js"></script><script type="text/javascript">	$(function() {		$('.list-luck').more({			'address' : '/member/luckAjax',			'amount' : 1		})	});</script><script type="text/javascript">	var provinceId = "${user.provinceId}";	var cityId = "${user.cityId}";	var areaId = "${user.areaId}";</script><script type="text/javascript" src="${mimeBase}/scripts/pub/common/area.js"></script>