<%@ page language="java" pageEncoding="UTF-8"%><!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"><html xmlns="http://www.w3.org/1999/xhtml"><head><meta http-equiv="Content-Type" content="text/html; charset=utf-8" /><%@ include file="../commons/commons_resource_head.jsp"%><title>${htmlTitleText }</title><meta name="keywords" content="${seoKeywords }" /><meta name="description" content="${seoDescription }" /></head><body><c:set var="nav" value="home"/><%@ include file="index_head.jsp"%>  <div class="banner">  <div class="banner1">   <div class="bd">    <ul>    <c:forEach items="${slides}" var="slide">     <li>      <a href="${slide.linkUrl}" style="background-image: url('${slide.photoPath}');"></a>     </li>    </c:forEach>    </ul>   </div>   <div class="hd">    <ul></ul>   </div>  </div>  <div class="screen-left menu">   <ul class="nav-sub">    <c:forEach items="${pcates}" var="pcate">    <li>     <a href="/product/list/${pcate.id }.html">      <em class="iconfontYC">${pcate.icons }</em>      <span>${pcate.name }</span>     </a>    </li>    </c:forEach>   </ul>  </div>  <div class="banner-hot box-sizing">   <div class="banner-top">热门推荐</div>   <div class="banner-hot1">    <div class="bd">     <ul>     <c:forEach items="${recomments}" var="recomment">      <li class="index1-img">       <em class="v scrollLoadingDiv">        <a href="/product/${recomment.id }.html" title="${recomment.name }" target="_blank">         <img src="${recomment.logoPath }"          id="buy_img_${recomment.id }" />        </a>        <span></span>       </em>       <p class="banner-title">        <a href="/product/${recomment.id }.html" title="${recomment.name }" target="_blank">(第${recomment.period}期)${recomment.name }</a>       </p>       <p class="banner-money">总需：${recomment.totalNum }人次</p>       <div class="progressBar">        <p class="progressBar-wrap">         <span style="width: <fmt:formatNumber value="${recomment.usedNum/recomment.totalNum*100}" pattern="0.0#"/>%"></span>        </p>        <div class="progressBar-txt">         <div class="txt-l">          <p>           <b>${recomment.usedNum }</b>          </p>          <p>已参与人次</p>         </div>         <div class="txt-r">          <p>           <b>${recomment.leaveNum }</b>          </p>          <p>剩余人次</p>         </div>        </div>       </div>       <div class="btn-go-box">        <a class="btn-go" href="javascript:void(0)" onclick="addToCart('${recomment.id}','buy')">立即云购</a>        <a class="btn-go btn-go-cart" href="javascript:void(0)" onclick="addToCart('${recomment.id}','',this,'01')"></a>       </div>      </li>      </c:forEach>     </ul>    </div>    <div class="hd">     <ul></ul>    </div>   </div>  </div> </div> <div class="index-ad" style="display:">  <div class="container">   <a href="/news/" class="ui-right">更多</a>   <strong class="ui-left">${comName}公告：</strong>   <div class="index-ad1">    <div class="bd">     <ul>     <c:forEach items="${news }" var="ns">      <li>       <a href="/news/${ns.id}.html" title="${ns.title }">        <span><fmt:formatDate value="${ns.createTime }" pattern="yyyy-MM-dd"/></span>        <p>${ns.title }</p>       </a>      </li>      </c:forEach>     </ul>    </div>   </div>  </div> </div>  <div class="container">  <dl class="index-top">   <dt>    <a href="/win/">查看更多</a>   </dt>   <dd>    <big>最新揭晓</big>   </dd>  </dl>  <ul class="index-new box-sizing" id="win-list">    </ul> </div>  <ul class="index-menu">  <li>   <a href="#section-1">    <em class="iconfontYC">&#xe618;</em>    <p>     <span>新品上线</span>    </p>   </a>  </li>       <c:forEach items="${floors}"  var="floor" varStatus="status">  <li>   <a href="#section-${status.count+1 }">    <em class="iconfontYC">${floor.icons }</em>    <p>     <span>${floor.name }</span>    </p>   </a>  </li>    </c:forEach>   <li>   <a href="#section-${fn:length(pcates)+1 }">    <em class="iconfontYC">&#xe604;</em>    <p>     <span>晒单分享</span>    </p>   </a>  </li>   </ul> <div class="index-menu-show">  <!-- floor_1 -->  <div class="container section" id="section-1">   <dl class="index-top">    <dt>     <a href="/product/new.html">查看更多</a>    </dt>    <dd>     <span class="iconfontYC">&#xe618;</span>     <big>新品上线</big>    </dd>   </dl>   <ul class="index1 box-sizing">      <c:forEach items="${newProducts}" var="item">    <li class="index1-img">     <em class="v scrollLoadingDiv">      <a href="/product/${item.id }.html" title="${item.name }" target="_blank">       <img class="scrollLoading" src="${ygBase}/style/images/pix.gif" data-url="${item.logoPath }"        id="buy_img_${item.id }" />      </a>      <span></span>     </em>     <big>      <a href="/product/${item.id }.html" title="${item.name }" target="_blank">(第${item.period }期)${item.name }</a>     </big>     <p>总需：${item.totalNum }  人次</p>    </li>    </c:forEach>   </ul>  </div>  <!-- end floor_1 -->    <c:forEach items="${floors}"  var="floor" varStatus="status">    <!-- floor_${status.count+1 } -->  <div class="container section" id="section-${status.count+1 }">   <dl class="index-top">    <dt>     <a href="/product/list/${floor.id}.html">查看更多</a>    </dt>    <dd>     <span class="iconfontYC">${floor.icons }</span>     <big>${floor.name }</big>    </dd>   </dl>   <ul class="index2 box-sizing">    <c:forEach items="${floor.products}"  var="product">    <li class="index1-img">     <em class="v scrollLoadingDiv">      <a href="/product/${product.id }.html" title="${product.name }" target="_blank">       <img class="scrollLoading" src="${ygBase}/style/images/pix.gif" data-url="${product.logoPath }"        id="buy_img_${product.id }" />      </a>      <span></span>     </em>     <p class="banner-title">      <a href="/product/${product.id }.html" title="${product.name }" target="_blank">(第${product.period }期)${product.name }</a>     </p>     <p class="banner-money">总需：${product.totalNum } 人次</p>     <div class="progressBar">      <p class="progressBar-wrap">       <span style="width: <fmt:formatNumber value="${product.usedNum/product.totalNum*100}" pattern="0.0#"/>%"></span>      </p>      <div class="progressBar-txt">       <div class="txt-l">        <p>         <b>${product.usedNum }</b>        </p>        <p>已参与人次</p>       </div>       <div class="txt-r">        <p>         <b>${product.leaveNum }</b>        </p>        <p>剩余人次</p>       </div>      </div>     </div>     <div class="btn-go-box">      <a class="btn-go" href="javascript:void(0)" onclick="addToCart('${product.id}','buy')">立即云购</a>      <a class="btn-go btn-go-cart" href="javascript:void(0)" onclick="addToCart('${product.id}','',this,'02')"></a>     </div>    </li>        </c:forEach>   </ul>  </div>    <!-- end floor_${status.count+1 } -->  </c:forEach>     <div class="index3" id="section-${fn:length(pcates)+1 }">  <div class="container">   <dl class="index-top">    <dt>     <a href="/share/">查看更多</a>    </dt>    <dd>     <span class="iconfontYC">&#xe604;</span>     <big>晒单分享</big>    </dd>   </dl>   <div class="index3-1">    <div class="bd">     <ul>         <c:forEach items="${shares}"  var="share">      <li>       <em>        <a href="/share/${share.id}.html" target="_blank">         <img  class="scrollLoading" src="${ygBase}/style/images/pix.gif" data-url="${share.photoList[0]}"/>        </a>       </em>       <div class="index3-2">        <big>         <a href="/share/${share.id}.html" target="_blank" title="12312">${share.title}</a>        </big>        <p>         <a href="/share/${share.id}.html" target="_blank" title="${share.content}">${share.content}</a>        <fmt:formatDate value="${share.createTime }" pattern="yyyy-MM-dd HH:mm"/>        </p>       </div>      </li>      </c:forEach>     </ul>    </div>   </div>  </div> </div><%@ include file="index_foot.jsp"%><script type="text/javascript" src="${mimeBase}/scripts/pub/index/newJiexiao.js"></script><script type="text/javascript"> //幻灯片 $(".banner1").slide({  titCell : ".hd ul",  mainCell : ".bd ul",  autoPage : true,  effect : "fold",  autoPlay : true,  interTime : 5000 }); //热门推荐 $(".banner-hot1").slide({  titCell : ".hd ul",  mainCell : ".bd ul",  autoPage : true,  effect : "leftLoop",  autoPlay : true,  interTime : 5000 }); //系统公告 $(".index-ad1").slide({  titCell : ".hd ul",  mainCell : ".bd ul",  autoPage : true,  effect : "topLoop",  autoPlay : true,  interTime : 5000 }); //晒单分享 $(".index3-1").slide({  titCell : ".hd ul",  mainCell : ".bd ul",  autoPage : true,  effect : "leftLoop",  autoPlay : true,  interTime : 5000,  vis : 4,  scroll : 4 });</script> <script type="text/javascript">  $(function() {  //漂浮左边楼层菜单  $('.index-menu').onePageNav({   filter : ':not(.external)',   scrollThreshold : 0.01  });   });</script> </body></html>